<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wxy.mapper.NoteClassMapper">

    <insert id="save" parameterType="noteClass" useGeneratedKeys="true" keyProperty="id">
        insert into sys_note_class(name)
        values(#{name})
    </insert>

    <select id="queryList" parameterType="noteClass" resultType="noteClass">
        select * from sys_note_class
    </select>
</mapper>